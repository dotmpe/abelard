{
    "name": "dotmpe/abelard",
    "description": "PHP cloudApp",
    "version": "0.0.1",
    "minimum-stability": "dev",
    "require": {
        "silex/silex": "*",
        "twig/twig": "*",
        "mheap/Silex-Assetic": "*",
        "natxet/CssMin": "*",
        "MSOpenTech/odataphpprod": "dev-master",
        "gizur/odatamysqlphpconnect": "dev-master"
    },
    "repositories": [
